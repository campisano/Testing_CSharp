<Window x:Class="NLayer.WPFMVP.LogImportView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LogImportView" Height="300" Width="300">
    <Grid>
        <Label Content="Path" HorizontalAlignment="Left" Margin="10,32,0,0" VerticalAlignment="Top"/>
        <TextBox IsEnabled="False" Name="xamlPath" HorizontalAlignment="Left" Height="23" Margin="55,32,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="144"/>
        <Button Content="Choose file" HorizontalAlignment="Left" Margin="210,32,0,0" VerticalAlignment="Top" Width="65" Click="OnChooseFile"/>
        <Label Content="Name" HorizontalAlignment="Left" Margin="10,78,0,0" VerticalAlignment="Top"/>
        <TextBox Name="xamlName" HorizontalAlignment="Left" Height="23" Margin="55,81,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="54"/>
        <Button Content="Import" HorizontalAlignment="Left" Margin="134,81,0,0" VerticalAlignment="Top" Width="65" Click="OnImport"/>
        <Label Content="Message" HorizontalAlignment="Center" Margin="10,135,0,0" VerticalAlignment="Top" Width="272"/>
        <TextBlock Name="xamlMessage" HorizontalAlignment="Left" Height="23" Margin="54,161,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="186"/>
    </Grid>
</Window>
