<Window x:Class="NLayer.WPFMVP.LogImportView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LogImportView" Height="300" Width="300">
    <Grid>
        <Label Content="Path" HorizontalAlignment="Left" Margin="54,32,0,0" VerticalAlignment="Top"/>
        <TextBox Name="xamlPath" HorizontalAlignment="Left" Height="23" Margin="120,32,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="162"/>
        <Label Content="Name" HorizontalAlignment="Left" Margin="54,78,0,0" VerticalAlignment="Top"/>
        <TextBox Name="xamlName" HorizontalAlignment="Left" Height="23" Margin="120,81,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="84"/>
        <Button Content="Import" HorizontalAlignment="Left" Margin="224,81,0,0" VerticalAlignment="Top" Width="44" Click="OnImport"/>
        <Label Content="Message" HorizontalAlignment="Left" Margin="120,135,0,0" VerticalAlignment="Top"/>
        <TextBlock Name="xamlMessage" HorizontalAlignment="Left" Height="23" Margin="54,161,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="186"/>
    </Grid>
</Window>
